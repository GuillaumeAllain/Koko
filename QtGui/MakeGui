#
# Separate make file to remove file debris and to install the
# executable. The main Makefile generated by qmake is removed after
# compilation because it contains system specific dependencies.
#


koko-gui:
	qmake
	make

.PHONY: clean install

clean:
	make clean
	rm -f Makefile KODS_GUI

install:
	mkdir -p $(PREFIX)/bin
	install -m 755 KODS_GUI $(PREFIX)/bin/koko-gui
